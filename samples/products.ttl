@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <https://schema.org/> .
@prefix ex: <http://example.org/catalog/> .
@prefix cat: <http://example.org/catalog/categories/> .

# Product Categories
cat:electronics a schema:Category ;
    schema:name "Electronics" ;
    rdfs:label "Electronics" ;
    schema:description "Electronic devices and gadgets" .

cat:books a schema:Category ;
    schema:name "Books" ;
    rdfs:label "Books" ;
    schema:description "Physical and digital books" .

cat:clothing a schema:Category ;
    schema:name "Clothing" ;
    rdfs:label "Clothing" ;
    schema:description "Apparel and fashion items" .

cat:home a schema:Category ;
    schema:name "Home & Garden" ;
    rdfs:label "Home & Garden" ;
    schema:description "Home improvement and garden supplies" .

# Products
